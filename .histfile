# Loop through all audio files in the current directory (you can modify the pattern to suit different audio formats)\
for file in *.mp3 *.flac *.wav *.m4a; do\
  # Skip non-audio files\
  if [ ! -f "$file" ]; then\
    continue\
  fi\
\
  # Extract metadata using ffprobe\
  artist=$(ffprobe -v error -show_entries format_tags=artist -of default=noprint_wrappers=1:nokey=1 "$file" 2>/dev/null || echo "Unknown Artist")\
  album=$(ffprobe -v error -show_entries format_tags=album -of default=noprint_wrappers=1:nokey=1 "$file" 2>/dev/null || echo "Unknown Album")\
\
  # Sanitize artist and album by replacing spaces and special characters\
  # Replace spaces with underscores and remove slashes, colons, etc.\
  artist=$(echo "$artist" | sed 's/[[:space:][:punct:]]/_/g')\
  album=$(echo "$album" | sed 's/[[:space:][:punct:]]/_/g')\
\
  # Ensure the strings are alphanumeric plus underscores (remove anything else)\
  artist=$(echo "$artist" | sed 's/[^a-zA-Z0-9_]//g')\
  album=$(echo "$album" | sed 's/[^a-zA-Z0-9_]//g')\
\
  # Create directories for artist and album if they don't exist\
  mkdir -p "$artist/$album"\
\
  # Move the song to the appropriate directory\
  mv "$file" "$artist/$album/"\
done\
\
echo "Music files have been copied to their respective directories."\
